{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "test-connectivity/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/test-connectivity",
      "dest": "/test-connectivity/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
} 