web: cd /app && find . -type d -name "__pycache__" -exec rm -rf {} + && find . -name "*.pyc" -delete && export DATABASE_URL=$(echo $DATABASE_URL | sed 's/postgresql:/postgresql+asyncpg:/') && python -m alembic upgrade head && uvicorn app.main:app --host 0.0.0.0 --port $PORT --workers ${UVICORN_WORKERS:-1} 